{
  "$schema": "https://app.flightcontrol.dev/schema.json",
  "environments": [
    {
      "id": "production",
      "name": "Production",
      "region": "us-west-2",
      "source": {
        "branch": "main"
      },
      "services": [
        {
          "id": "my-static",
          "name": "My Static Site",
          "type": "static",
          "domain": "www.example.com",
          "buildCommand": "nuxt generate",
          "outputDirectory": ".output/public",
          "singlePageApp": false
        }
      ]
    }
  ]
}
